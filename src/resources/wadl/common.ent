<!ENTITY sessionPersistenceParameter '
<param xmlns="http://wadl.dev.java.net/2009/02"
    name="sessionPersistence" style="plain" required="false"
    type="xsd:string">
    <wadl:doc xmlns="http://docbook.org/ns/docbook"
        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
        <para>Specifies whether multiple requests from clients are
            directed to the same node.</para>
    </wadl:doc>
</param>'>
<!ENTITY httpsRedirectParameter '
<param xmlns="http://wadl.dev.java.net/2009/02" name="httpsRedirect"
    style="plain" required="false" type="xsd:boolean">
    <wadl:doc xmlns="http://docbook.org/ns/docbook"
        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
        <para>Enables or disables HTTP to HTTPS redirection for the
            load balancer. When enabled, any HTTP request returns
            status code 301 (Moved Permanently), and the requester is
            redirected to the requested URL via the HTTPS protocol on
            port 443. For example, <link
                xlink:href="http://example.com/page.html"
                >http://example.com/page.html</link> would be
            redirected to <link
                xlink:href="https://example.com/page.html"
                >https://example.com/page.html</link>. Only available
            for HTTPS protocol (<code>port=443</code>), or HTTP
            protocol with a properly configured SSL termination
                (<code>secureTrafficOnly=true</code>,
                <code>securePort=443</code>).</para>
    </wadl:doc>
</param>'>
<!ENTITY keyParameter '
<param name="key" style="plain" required="true" type="xsd:string">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
        <para>Key that is used to identify the metadata. Must be 256
            characters or less. All UTF-8 characters are valid.</para>
    </wadl:doc>
</param>
'>
<!ENTITY valueParameter '
<param name="value" style="plain" required="true" type="xsd:string">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
