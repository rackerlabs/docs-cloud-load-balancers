                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/json" xml:lang="EN">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN">
                    <xsdxt:code
                        href="../samples/lb-create-required-sharedip-request.json"
                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/xml" xml:lang="EN">
                <wadl:doc xmlns="http://docbook.org/ns/docbook"
                    xml:lang="EN">
                    <xsdxt:code
                        href="../samples/lb-create-required-sharedip-request.xml"
                    />
                </wadl:doc>
            </representation>
            <representation mediaType="application/json" xml:lang="EN">
